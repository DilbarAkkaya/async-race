{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "airbnb/base",
      "eslint:recommended",
      "plugin:eslint-plugin/recommended"
  ],
  "ignorePatterns": ["webpack.config.js"],
    "parser":"@babel/eslint-parser",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
  },
  "plugins": [
      "eslint-plugin"
  ],
  "rules": {
    "no-shadow": "off",
    "no-plusplus": "off",
    "no-console": "error",
    "max-len": ["warn", { "code": 128 }],
    "indent": ["warn", 2, {
      "SwitchCase": 1
    }],
    "import/prefer-default-export": "off",
    "no-param-reassign": ["error", {
      "props": false
    }]
  }
}