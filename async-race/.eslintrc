{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "airbnb/base",
      "eslint:recommended",
      "plugin:eslint-plugin/recommended"
  ],
  //"parser": "eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "eslint-plugin"
  ],
  "rules": {
    "no-shadow": "off",
    "no-plusplus": "off",
    "no-console": "warn",
    "max-len": ["warn", { "code": 128 }],
    "indent": ["warn", 2, {
      "SwitchCase": 1
    }],
/*     "eslint/indent": ["warn", 2, {
      "SwitchCase": 1
    }], */
    "import/prefer-default-export": "off",
    "no-param-reassign": ["error", {
      "props": false
    }]
  }
}